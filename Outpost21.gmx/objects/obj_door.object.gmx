            <string>///draw based on direction placed
if image_angle == 0 or image_angle == 180 {
    draw_sprite(spr_door_stationA,1,x,y+32);
    if security_color != -1 draw_sprite_ext(spr_door_securitylight,1,x,y+32,1,1,0,security_color,1);
}
if image_angle == 90 or image_angle == 270 {
    draw_sprite(spr_door_panels_stationA,1,x+panel_slide,y+8);
    draw_sprite(spr_door_panels_stationA,0,x-panel_slide,y+8);
    draw_sprite(spr_door_stationA,0,x,y+8);
    if security_color != -1 draw_sprite_ext(spr_door_securitylight,0,x,y+8,1,1,0,security_color,1);
}


//proximity
if alarm[0] &gt; 0 { //if the door was already open and a player is by it, or the alarm is setting it open!
    if panel_slide &lt; 15.90 {
        panel_slide = lerp(panel_slide,16,0.10);
    }
    else
    {
        panel_slide = 16;
    }
}
else
{
    if panel_slide &gt; 0.10 {
        panel_slide = lerp(panel_slide,0,0.10);
    }
    else
    {
        panel_slide = 0;
    }
}   


